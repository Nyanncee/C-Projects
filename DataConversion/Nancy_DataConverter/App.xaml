<Application x:Class="DataConverters.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DataConverters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:InstructorConverter x:Key="InstructorConverter"/>
        <local:OpenConverter x:Key="OpenConverter"/>
        <local:addConverter x:Key="addConverter"/>
    </Application.Resources>
</Application>
